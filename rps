import random
computer= random.choice(["rock", "paper", "scissor"])
user= input("Enter you choice"). lower()
print(f"Computer choose: {computer}, You choose: {user}")

if computer==user:
    print("Draw")
else:
    if computer=="rock" and user=="paper":
        print("You win")
    elif computer=="rock" and user=="scissor":
        print("Computer wins")
    elif computer=="paper" and user=="scissor":
        print("You win")
    elif computer=="paper" and user=="rock":
        print("computer wins")
    elif computer=="scissor" and user=="paper":
        print("Computer wins")
    elif computer=="scissor" and user=="rock":
        print("You win")

    else:
        print("Something went wrong")

